.content {}

.left-bar {
    width: 60vw;
    padding: 10rpx 50rpx;
    box-sizing: border-box;
}

.left-bar-title{
    font-weight: 600;
}

.item-types{
    margin: 20rpx 0;
}

.item-type{
    height: 60rpx;
    width: calc(100% - 40rpx);
    line-height: 60rpx;
    border: 1px solid #d2d2d2;
    border-radius: 6rpx;
    padding: 0 20rpx;
    margin: 10rpx 0;
    font-size: 14px;
}

