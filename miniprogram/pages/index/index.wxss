
/* 主体内容开始 */
.content {}
/* -- 总数统计 -- */
.content .total-list {
  background: white;
  box-shadow: 0 8px 16px rgba(102,102,102,0.11);
  padding: 30rpx 0;
  display: flex;
  align-items: center;
}
.total-list .item {
  flex: 1;
  text-align: center;
  display: inline-block;
}
.total-list .item .num {
  font-size: 50rpx;
  font-weight: bold;
}
.total-list .item .text {
  font-size: 24rpx;
  font-weight: bold;
  color: #424242;
  padding-top: 20rpx;
}

